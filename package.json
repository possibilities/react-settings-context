{
  "name": "react-settings-context",
  "version": "0.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm-run-all --race --parallel \"dev:test -w\" dev:lint \"dev:run -- {@}\" --",
    "dev:run": "NODE_ENV=production rollup -c -w",
    "dev:lint": "sleep 5 && yarn lint-watch",
    "dev:test": "sleep 3 && yarn test",
    "release": "release",
    "release:publish": "npm publish",
    "prepublishOnly": "yarn build",
    "build": "NODE_ENV=production rollup -c",
    "test": "ava --verbose",
    "lint": "standard",
    "lint-watch": "nodemon --quiet --exec 'standard && echo linting: OK' --watch '**/*.js' --ignore node_modules"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "ava": "^1.3.1",
    "release": "^6.0.1",
    "rollup": "^1.5.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "standard": "^12.0.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
}
